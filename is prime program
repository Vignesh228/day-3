import.java.io.*;
import.java.util.*;
class UserMainCode1
{
  public static int isPrime(int input1)
{
      int count=0;
      for(int i=1;i<=input1;i++)
      {
           if(input1%i==0)
           {
               count++;
           }
      }
      if(count==2)
      {
          return 2;
      }
      else
      {
         return 1;
      }
   }
   public static void main(String args[])
   {
     int num,result;
    Scanner sc=new Scanner(system.in);
     System.out.println("enter the number");
      num=sc.nextInt();
      result=isPrime(num);
      System.out.println(result);
}
}


